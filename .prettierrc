{
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 4,
  "semi": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "useTabs": true,
  "printWidth": 100,
  "proseWrap": "never",
  "arrowParens": "avoid",
  "jsxSingleQuote": true,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ]
}